{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "ES6",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "skipLibCheck": true,
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        // exclude node_modules
        "./node_modules/**/*"
    ],
}